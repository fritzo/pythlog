

def f0():
	pass

def f1(a, b, c):
	pass

def f2(a, b, c):
	return a + b + c

def test_call_function_without_arguments_and_return_value():
	assert None == f0()

def test_call_function_with_arguments_and_without_return_value():
	assert None == f1(10, 20, 30)

def test_call_function_with_arguments_and_return_value():
	x = f2(10, 20, 30)
	assert 60 == x