
def test_addition():
	assert 10 + 20 == 30, "10 + 20 == 30"
	assert 1 + 200 == 201, "1 + 200 == 201"
	assert (362974238643794838798219 + 4382748931784713898791379843
			== 4383111906023357693630178062)

def test_subtraction():
	assert 10 - 20 == -10, "10 - 20 == -10"
	assert 1 - 200 == -199, "1 - 200 == -199"
	assert (77482379489387498327490 - 47238748379483728497892374983
			== -47238670897104239110394047493)

def test_multiplication():
	assert 10 * 20 == 200, "10 * 20 == 100"
	assert 13 * -10 == -130, "13 * -10 == -130"
	assert (578392470575584295 * 85472565405376542
			== 49436688271248955708133786136607890)

def test_division():
	assert 10 / 2 == 5, "10 / 2 == 5"
	assert 130 / -10 == -13, "130 / -10 == -13"
	assert (875489758475847985749375948 / 54357847895743
			== 16106041581245)

def test_modulo():
	assert 10 % 3 == 1, "10 % 3 == 1"
	assert -5 % 3 == 1, "-5 / 3 == 1"
	assert 5 % -3 == -1, "5 / -3 == -1"
	assert 7548937589475984795843 % 43274923 == 1708748

def test_exponential():
	assert 2 ** 4 == 16, "2 ** 4 == 16"
	assert 3 ** 2 == 9, "3 ** 2 == 9"
	assert 43 ** 15 == 3177070365797955661914307